<svg xmlns="http://www.w3.org/2000/svg" width="800" height="400" viewBox="0 0 800 400">
  <rect width="800" height="400" fill="#1a1b26" rx="10" ry="10"/>
  <g transform="translate(400, 200)">
    <!-- Dashboard Container -->
    <rect x="-300" y="-150" width="600" height="300" rx="10" ry="10" stroke="#4b31dd" stroke-width="3" fill="#2a2c3d"/>
    
    <!-- Dashboard Header -->
    <rect x="-300" y="-150" width="600" height="50" rx="10" ry="10" fill="#141521"/>
    <text x="0" y="-120" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-weight="bold" font-size="20">Privacy Dashboard</text>
    
    <!-- Tracker Counter -->
    <g transform="translate(-200, -50)">
      <rect x="-70" y="-30" width="140" height="110" rx="5" ry="5" fill="#1a1b26"/>
      <text x="0" y="-5" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="14">Trackers Blocked</text>
      <text x="0" y="30" text-anchor="middle" fill="#4b31dd" font-family="Arial" font-weight="bold" font-size="36">2,547</text>
      <text x="0" y="60" text-anchor="middle" fill="#8673f4" font-family="Arial" font-size="12">Last 7 Days</text>
    </g>
    
    <!-- Tracker Categories -->
    <g transform="translate(50, -50)">
      <rect x="-150" y="-30" width="300" height="110" rx="5" ry="5" fill="#1a1b26"/>
      <text x="0" y="-10" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="14">Tracker Categories</text>
      
      <!-- Bar Chart -->
      <rect x="-120" y="10" width="80" height="10" fill="#4b31dd"/>
      <rect x="-120" y="30" width="60" height="10" fill="#8673f4"/>
      <rect x="-120" y="50" width="40" height="10" fill="#4b31dd"/>
      <rect x="-120" y="70" width="20" height="10" fill="#8673f4"/>
      
      <!-- Legend -->
      <text x="-30" y="18" text-anchor="start" fill="#f8f8f2" font-family="Arial" font-size="12">Advertising (45%)</text>
      <text x="-30" y="38" text-anchor="start" fill="#f8f8f2" font-family="Arial" font-size="12">Analytics (32%)</text>
      <text x="-30" y="58" text-anchor="start" fill="#f8f8f2" font-family="Arial" font-size="12">Social Media (18%)</text>
      <text x="-30" y="78" text-anchor="start" fill="#f8f8f2" font-family="Arial" font-size="12">Other (5%)</text>
    </g>
    
    <!-- Top Tracking Sites -->
    <g transform="translate(-200, 90)">
      <rect x="-70" y="-30" width="140" height="90" rx="5" ry="5" fill="#1a1b26"/>
      <text x="0" y="-10" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="14">Top Sites</text>
      <text x="0" y="15" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="10">example-tracker.com</text>
      <text x="0" y="35" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="10">ads-network.com</text>
      <text x="0" y="55" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="10">social-tracker.net</text>
    </g>
    
    <!-- Timeline -->
    <g transform="translate(50, 90)">
      <rect x="-150" y="-30" width="300" height="90" rx="5" ry="5" fill="#1a1b26"/>
      <text x="0" y="-10" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="14">Tracking Attempts</text>
      
      <!-- Simple Line Chart -->
      <polyline points="-120,40 -80,20 -40,35 0,15 40,25 80,10 120,30" fill="none" stroke="#4b31dd" stroke-width="2"/>
      
      <!-- X Axis -->
      <line x1="-120" y1="50" x2="120" y2="50" stroke="#2c2f45" stroke-width="1"/>
      <text x="-120" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Mon</text>
      <text x="-80" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Tue</text>
      <text x="-40" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Wed</text>
      <text x="0" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Thu</text>
      <text x="40" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Fri</text>
      <text x="80" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Sat</text>
      <text x="120" y="65" text-anchor="middle" fill="#f8f8f2" font-family="Arial" font-size="8">Sun</text>
    </g>
  </g>
</svg> 